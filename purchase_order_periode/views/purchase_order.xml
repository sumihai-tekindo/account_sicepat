<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_order_purchase_inherited" model="ir.ui.view">
			<field name="name">purchase.order.tree.inherited</field>
			<field name="model">purchase.order</field>
			<field name="inherit_id" ref="purchase.purchase_order_form"/>
			<field name="arch" type="xml">
				<field name="currency_id" position="after">
					<field name="date_start"/>
					<field name="date_end"/>
				</field>
			</field>
		</record>
	</data>
	
		<data>
		<record id="view_account_invoice_inherited" model="ir.ui.view">
			<field name="name">account.invoice.tree.inherited</field>
			<field name="model">account.invoice</field>
			<field name="inherit_id" ref="account.invoice_supplier_form"/>
			<field name="arch" type="xml">
				<field name="supplier_invoice_number" position="after">
					<field name="date_start"/>
					<field name="date_end"/>
				</field>
			</field>
		</record>
	</data>
</openerp>
